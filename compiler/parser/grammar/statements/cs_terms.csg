
TERM: LITERAL | identifier | FUNCTION_INVOCATION | ARRAY_ACCESSOR | PART_OF_ACCESSOR | ATTRIBUTE_ACCESSOR

LITERAL: null
         | int_literal | int_binary_literal | int_hex_literal
         | float_literal
         | double_literal
         | boolean_literal
         | char_literal
         | string_literal
         | ARRAY_LITERAL
         | TUPLE_LITERAL

ARRAY_LITERAL: ARRAY_LITERAL_1D | ARRAY_LITERAL_2D | ARRAY_LITERAL_3D

TUPLE_LITERAL: ( COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST )

ARRAY_LITERAL_1D: [ COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ]
ARRAY_LITERAL_2D: [ COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ] [ COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ]
ARRAY_LITERAL_3D: [ COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ] [ COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ] [ COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ]

FUNCTION_INVOCATION: identifier ( COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST ) | TERM . identifier ( COMMA_SEPARATED_NON_ASSIGNMENT_EXPRESSION_LIST )

ARRAY_ACCESSOR: TERM [ NON_ASSIGNMENT_EXPRESSION ]

PART_OF_ACCESSOR: TERM [ identifier ]

ATTRIBUTE_ACCESSOR: TERM . identifier


